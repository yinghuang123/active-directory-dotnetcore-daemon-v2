[remote "Azure-Samples"]
    url = git@github.com:Azure-Samples/active-directory-dotnetcore-daemon-v2.git
    fetch = +refs/heads/*:refs/remotes/Azure-Samples/*
[remote "origin"]
    url = git@github.com:yinghuang123/active-directory-dotnetcore-daemon-v2.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = Azure-Samples
    target = Azure-Samples/master
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/workspace/Azure-Samples/active-directory-dotnetcore-daemon-v2
